# ===============================
# üåê BASIC PROJECT CONFIGURATION
# ===============================
REACT_APP_ENV=development
REACT_APP_VERSION=1.0.0

# ===============================
# üî• FIREBASE CONFIGURATION
# ===============================
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=000000000000
REACT_APP_FIREBASE_APP_ID=1:000000000000:web:xxxxxxxxxxxxxxxxxxxx
REACT_APP_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# ===============================
# ü§ñ AI SERVICES
# ===============================

# Google Vision API ‚Äì brukes for gratis auto-tagging og analyse
REACT_APP_GOOGLE_VISION_KEY=your_google_vision_api_key_here

# Picsart API ‚Äì brukes for Pro-funksjoner (remove BG, enhance, effects)
REACT_APP_PICSART_KEY=your_picsart_api_key_here

# Gemini / ChatGPT ‚Äì brukes for prompt-forsterkning og fremtidig AI-assistent
REACT_APP_GEMINI_KEY=your_gemini_api_key_here
REACT_APP_OPENAI_KEY=your_openai_api_key_here

# ===============================
# ‚öôÔ∏è FIREBASE CLOUD FUNCTIONS (AI Proxy)
# ===============================
REACT_APP_AI_PROXY_URL=https://your-region-yourproject.cloudfunctions.net

# ===============================
# üß† DEBUG & LOCAL DEV SETTINGS
# ===============================
REACT_APP_LOG_LEVEL=debug
REACT_APP_ENABLE_MOCK_AI=false
